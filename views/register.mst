{{>header}}

<h1>Faca o seu Cadastro</h1>

<form method="POST">
    
    <label>
        Nome:
        <input type="text" name="name"/>
    </label>
    <br/>

    <label>
        E-mail:
        <input type="text" name="email"/>
    </label>
    <br/>

    <label>
    Senha:
        <input type="password" name="password"/>
    </label>
    <br/>
    
    <label>
    
        <input type="submit" name="Cadastrar"/>
    </label>


</form>